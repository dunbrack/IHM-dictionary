;
    Example 1 -
;
;
	_ihm_modeling_experiment.ordinal_id			1
	_ihm_modeling_experiment.id				1
	_ihm_modeling_experiment.struct_assembly_id		1
	_ihm_modeling_experiment.assembly_description		'Nup84 sub-complex'
	_ihm_modeling_experiment.sampled_models			15000
	_ihm_modeling_experiment.sampling_method                'Replica exchange monte carlo'
	_ihm_modeling_experiment.filtered_models		6520
	_ihm_modeling_experiment.filtering_parameter		'energy/score'
	_ihm_modeling_experiment.clustered_models		6520
	_ihm_modeling_experiment.clustering_parameter		'dRMSD'
